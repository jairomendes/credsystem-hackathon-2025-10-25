# tensorflow: Usa similaridade de texto com dados de treinamento (rápido, sem dependências)
CLASSIFIER_TYPE=openrouter

# Caminho dos dados de treinamento
TRAINING_DATA_PATH=./training/intents_pre_loaded.csv

